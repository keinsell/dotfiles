{
  "workbench.colorTheme": "Ayu Light Bordered",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.autoSave": "onWindowChange",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.iconTheme": "quill-icons",
  "workbench.editor.showTabs": false,
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "problems.showCurrentInStatus": true,
  "editor.cursorSmoothCaretAnimation": true,
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "jshint.packageManager": "yarn",
  "audioCues.volume": 90,
  "audioCues.lineHasWarning": "auto",
  "explorer.confirmDragAndDrop": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "notebook.outline.showCodeCells": true,
  "outline.showArrays": false,
  "outline.showBooleans": false,
  "outline.showConstants": false,
  "outline.showEvents": false,
  "outline.showFields": false,
  "outline.showFiles": false,
  "outline.showKeys": false,
  "outline.showNull": false,
  "outline.showNumbers": false,
  "outline.showObjects": false,
  "outline.showOperators": false,
  "outline.showProperties": false,
  "outline.showStrings": false,
  "outline.showTypeParameters": false,
  "outline.showVariables": false,
  "editor.inlineSuggest.enabled": true,
  "redhat.telemetry.enabled": true,
  "editor.wordWrap": "bounded",
  "editor.wrappingIndent": "indent",
  "editor.minimap.maxColumn": 80,
  "security.workspace.trust.untrustedFiles": "open",
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "search.showLineNumbers": true,
  "rome.analysis.enableCodeActions": true,
  "rome.analysis.enableDiagnostics": true,
  "rome.formatter.formatWithSyntaxErrors": true,
  "rome.unstable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.run": "onSave",
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.semicolons": "remove",
  "editor.suggest.preview": true,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "markdown.preview.typographer": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "workbench.colorCustomizations": {},
  "settingsSync.ignoredSettings": [],
  "editor.accessibilitySupport": "off",
  "workbench.startupEditor": "none",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.fontFamily": "'Jetbrans Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.renderWhitespace": "boundary",
  "workbench.editor.enablePreview": false,
  "workbench.preferredLightColorTheme": "Ayu Light Bordered",
  "workbench.preferredDarkColorTheme": "Ayu Mirage Bordered",
  "window.autoDetectColorScheme": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.inlayHints.parameterNames.enabled": "literals",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "github.codespaces.connectionType": "any",
  "github.codespaces.showPerformanceExplorer": true,
  "remoteHub.experimental.useDiffMatchPatch": true,
  "editor.cursorBlinking": "smooth",
  "workbench.reduceMotion": "off",
  "editor.parameterHints.cycle": true,
  "zenMode.hideTabs": false,
  "editor.tabCompletion": "on",
  "editor.suggest.shareSuggestSelections": true
}
