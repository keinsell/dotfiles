{
	"workbench.colorTheme": "Min Light",
	"settingsSync.ignoredExtensions": ["msjsdiag.debugger-for-chrome"],
	"editor.inlineSuggest.enabled": true,
	"workbench.productIconTheme": "icons-carbon",
	"prettier.endOfLine": "auto",
	"prettier.jsxSingleQuote": true,
	"prettier.proseWrap": "always",
	"prettier.singleQuote": true,
	"prettier.tabWidth": 3,
	"prettier.trailingComma": "none",
	"prettier.useTabs": true,
	"files.autoSave": "onFocusChange",
	"editor.tabSize": 3,
	"editor.tabCompletion": "on",
	"workbench.editor.showTabs": false,
	"workbench.editor.tabCloseButton": "off",
	"editor.wordWrap": "bounded",
	"yaml.format.proseWrap": "always",
	"editor.cursorSmoothCaretAnimation": true,
	"workbench.list.smoothScrolling": true,
	"editor.smoothScrolling": true,
	"editor.formatOnPaste": true,
	"files.autoGuessEncoding": true,
	"workbench.preferredDarkColorTheme": "Min Dark",
	"workbench.preferredLightColorTheme": "Min Light",
	"git.autofetch": true,
	"git.autoStash": true,
	"javascript.preferences.importModuleSpecifier": "relative",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"timeline.pageOnScroll": true,
	"explorer.experimental.fileNesting.enabled": true,
	"explorer.incrementalNaming": "smart",
	"workbench.fontAliasing": "auto",
	"workbench.editor.enablePreviewFromCodeNavigation": true,
	"workbench.editor.enablePreviewFromQuickOpen": true,
	"editor.wrappingIndent": "indent",
	"editor.wrappingStrategy": "advanced",
	"editor.wordWrapColumn": 160,
	"githubPullRequests.fileListLayout": "flat",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"window.autoDetectColorScheme": true,
	"workbench.iconTheme": "vs-minimal",
	"search.smartCase": true,
	"search.collapseResults": "auto",
	"workbench.sideBar.location": "right",
	"typescript.tsserver.enableTracing": true,
	"typescript.tsserver.experimental.enableProjectDiagnostics": true,
	"typescript.tsserver.trace": "messages",
	"typescript.tsserver.log": "off",
	"js/ts.implicitProjectConfig.experimentalDecorators": true,
	"js/ts.implicitProjectConfig.checkJs": true,
	"editor.hover.above": false,
	"redhat.telemetry.enabled": true,
	"javascript.updateImportsOnFileMove.enabled": "always"
}
