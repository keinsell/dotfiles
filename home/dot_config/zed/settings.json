{
  "base_keymap": "VSCode",
  "vim_mode": false,
  "buffer_font_size": 15,
  "preferred_line_length": 80,
  "show_copilot_suggestions": true,
  "theme": {
    "mode": "system",
    "dark": "Ayu Mirage",
    "light": "Ayu Mirage"
  },
  "formatter": "language_server",
  "show_whitespaces": "selection",
  "show_completions_on_input": true,
  "autosave": "on_focus_change",
  "cursor_blink": true,
  "cursor_style": "block",
  "relative_line_numbers": true,
  "active_pane_magnification": 3,
  "enable_language_server": true,
  "auto_install_extensions": {},
  "file_scan_exclusions": [
    "node_modules",
    "bower_components",
    "vendor",
    "dist",
    "build",
    "out",
    "target",
    "tmp",
    "temp",
    ".*",
    "*.log",
    "*.min.js",
    "*.min.css",
    "*.map",
    "*.gz",
    "*.tar",
    "*.zip",
    "*.rar",
    "*.7z",
    "*.exe",
    "*.dll",
    "*.obj",
    "*.o",
    "*.a",
    "*.lib",
    "*.so",
    "*.dylib",
    "*.bin",
    "*.eot",
    "*.ttf",
    "*.woff",
    "*.woff2",
    "*.ico",
    "*.jpg",
    "*.jpeg",
    "*.png",
    "*.gif",
    "*.svg",
    "*.mp3",
    "*.mp4",
    "*.webm",
    "*.avi",
    "*.flv",
    "*.wmv",
    "*.mov",
    "*.mkv",
    "*.webp",
    "*.pdf",
    "*.doc",
    "*.docx",
    "*.xls",
    "*.xlsx",
    "*.ppt",
    "*.pptx",
    "*.key",
    "*.odt",
    "*.ods",
    "*.odp",
    "*.md",
    "*.markdown",
    "*.txt",
    "*.rtf",
    "*.csv",
    "*.json",
    "*.xml",
    "*.toml",
    "*.ini",
    "*.cfg",
    "*.conf",
    "*.config",
    "*.env",
    "*.lock",
    "*.log",
    "*.tmp",
    "*.temp",
    "*.bak",
    "*.swp"
  ],
  "extend_comment_on_newline": true,
  "redact_private_values": true,
  "use_autoclose": true,
  "assistant": {
    "enabled": true,
    "version": "1"
  },
  "features": {
    "copilot": true
  },
  "centered_layout": {
    "left_padding": 0.05,
    "right_padding": 0.05
  },
  "hover_popover_enabled": true,
  "hover_popover_delay": 0.0001,
  "format_on_save": "on",
  "show_call_status_icon": true,
  "assistant_v2": {
    "enabled": true
  },
  "scrollbar": {
    "show": "auto",
    "cursors": true,
    "git_diff": true,
    "search_results": true,
    "selected_symbol": true,
    "diagnostics": true
  },
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": false,
      "delay_ms": 1000
    }
  },
  "inlay_hints": {
    "enabled": true,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true,
    "edit_debounce_ms": 0,
    "scroll_debounce_ms": 50
  },
  "preview_tabs": {
    "enabled": true,
    "enable_preview_from_file_finder": true,
    "enable_preview_from_code_navigation": true
  },
  "soft_wrap": "preferred_line_length",
  "message_editor": {
    "auto_replace_emoji_shortcode": true
  },
  "tabs": {}
}
