{
	"editor.showInlayHints": true,
	"theme": "islands_dark",
	"editor.fontSize": 12,
	"editor.ligatures": true,
	"editor.softWrapEnabled": true,
	"editor.renderWhitespace": "All",
	"editor.formatOnSave": true,
	"editor.caretSmoothAnimation": true,
	"editor.showFloatingDebugPanel": true,
	"plugins": [
		{
			"type": "add",
			"pluginName": "com.github.catppuccin.fleet"
		},
		{
			"type": "add",
			"pluginName": "gradle.daemon.services"
		}
	],
	"backend.enable.k2": true,
	"backend.fsdMode": true,
	"backend.noDotIdea": false,
	"collapseMacroOnSave": true,
	"editor.alwaysShowVerticalScrollbar": false,
	"editor.centerEditorHorizontally": true,
	"editor.distractionFreeMode": true,
	"editor.indentStyle": "Tabs",
	"editor.showAiPrompt": true,
	"editor.guideToSoftWrapBy": 80,
	"fus.ai.sendAllowed": true,
	"ff.localStatusBar": true,
	"ff.previewAndPrioritiseFileSymbolsInGotoEverywhere": true,
	"ff.runIconsAfterLineNumbers": true,
	"ff.showDaemonPostlines": true,
	"ff.showShortcutPreview": true,
	"ff.useJediTerm": true,
	"ff.yamlEnableDeindentingEnterHandlers": true,
	"fontHinting": "Full",
	"preferredRenderingApi": "METAL",
	"launchTerminalOnOpen": true,
	"terminal.TypeAheadLatency": 100,
	"completion.autoPopupDelay": 0,
	"quickFixes.preview.contextLines": 3,
	"rename.autoRenameOnTyping": true,
	"rust-analyzer.assist.emitMustUse": true,
	"rust-analyzer.cargo.features": [
		"all"
	],
	"rust-analyzer.check.command": "clippy",
	"rust-analyzer.diagnostics.experimental.enable": true,
	"rust-analyzer.hover.actions.references.enable": true,
	"rust-analyzer.inlayHints.bindingModeHints.enable": true,
	"rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
	"rust-analyzer.inlayHints.typeHints.hideClosureInitialization": true,
	"rust-analyzer.interpret.tests": true,
	"rust-analyzer.lens.references.adt.enable": true,
	"rust-analyzer.lens.references.enumVariant.enable": true,
	"rust-analyzer.lens.references.method.enable": true,
	"rust-analyzer.lens.references.trait.enable": true,
	"rust-analyzer.lru.capacity": 512,
	"rust-analyzer.references.excludeImports": true,
	"rust-analyzer.rustfmt.rangeFormatting.enable": true,
	"rust-analyzer.semanticHighlighting.operator.specialization.enable": true,
	"rust-analyzer.semanticHighlighting.punctuation.separate.macro.bang": true,
	"rust-analyzer.semanticHighlighting.punctuation.specialization.enable": true,
	"rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
}